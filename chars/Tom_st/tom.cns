trigger1 = animelem = 3
value = s68,1

[State 900, 12]
type = PlaySnd
trigger1 = animelem = 6
value = s68,2

[State 900, 4]
type = gamemakeanim
trigger1 = animelem = 3
value = 2
pos = 23, -49

[State 900, 4]
type = gamemakeanim
trigger1 = animelem = 6
value = 3
pos = 23, -49



[State 810, State End]
