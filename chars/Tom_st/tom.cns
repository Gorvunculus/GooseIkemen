type = HitDef
trigger1 = AnimElem = 3
attr = S, NA
animtype  = Medium
damage    = 67
guardflag = M
pausetime = 12,12
sparkno = s2010 ;1
sparkxy = -10,-23
hitsound   = s660,0
guardsound = 6,0
ground.type = High
ground.slidetime = 12
ground.hittime  = 15
ground.velocity = -5.5
air.velocity = -2.5,-4

[State 1305, 2.9]
